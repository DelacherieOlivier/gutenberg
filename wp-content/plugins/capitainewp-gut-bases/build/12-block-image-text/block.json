{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "capitainewp/block-image-text",
  "version": "1.0",
  "title": "12. block-image-text",
  "icon": "admin-appearance",
  "category": "text",
  "description": "Un bloc avec du joli CSS",
  "supports": {
    "html": false
  },
  "attributes": {
    "number": {
      "type": "string",
      "source": "html",
      "selector": ".number"
    },
    "title": {
      "type": "string",
      "source": "html",
      "selector": ".title"
    },
    "pictureID": {
      "type": "number",
      "default": null
    },
    "pictureURL": {
      "type": "string",
      "source": "attribute",
      "attribute": "src",
      "selector": "img"
    },
    "pictureAlt": {
      "type": "string",
      "source": "attribute",
      "attribute": "alt",
      "selector": "img"
    }
  },
  "textdomain": "capitainewp-gut-bases",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}